<section class="py-7 bg-light" id="faq">
    <div class="container">
        <div class="row">
            <div class="col-md-6 mx-auto text-center">
                <span class="text-muted text-uppercase">{{subtitle}}</span>
                <h2 class="display-4">{{title}}</h2>
            </div>
        </div>
        <div class="row mt-5">
            <div class="col-md-9 mx-auto">
                <div class="row mt-4">
                    {{#each items}}
                    <div class="col-md-6 mb-4">
                        <h5>{{title}}</h5>
                        <p class="text-muted">
                            {{text}}
                        </p>
                    </div>
                    {{/each}}
                </div>
                <div class="row text-center my-5">
                    <div class="col-lg-6 col-md-8 mx-auto">
                        <div class="font-weight-bold lead">{{moreTitle}}</div>
                        <p class="text-muted">{{moreText}}</p>
                        <a href="#" class="btn btn-primary btn-sm">
                            {{moreLink.label}}
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>