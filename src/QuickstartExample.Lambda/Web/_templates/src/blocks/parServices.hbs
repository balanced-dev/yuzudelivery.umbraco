<section class="py-7 bg-light" id="services">
    <div class="container">
        <div class="row">
            <div class="col-md-7 col-sm-9 mx-auto text-center">
                <span class="text-muted text-uppercase">{{subtitle}}</span>
                <h2 class="display-4">{{title}}</h2>
                <p class="lead">{{subtitle}}</p>
            </div>
        </div>
        <div class="row p-4 mt-5 bg-white raised-box rounded">
            {{#each items}}
            <div class="col-md-4 col-sm-6 mb-5 text-center">
                <div class="icon-box">
                    <div class="icon-box-inner small-xs text-primary">
                        <span data-feather="{{icon}}" width="30" height="30"></span>
                    </div>
                </div>
                <h4 class="mt-2">{{title}}</h4>
                <p class="text-muted">{{text}}</p>
            </div>
            {{/each}}
        </div>
        <div class="row press mt-5">
            <div class="col-12 text-muted small-xl text-uppercase text-center mb-5">{{expTitle}}
            </div>
            {{#each expItems}}
            <div class="press-item col-lg-2 col-md-4 col-6"><a href="#"><img class="img-fluid"
                        src="{{image.src}}" alt=""></a></div>
            {{/each}}
        </div>
    </div>
</section>